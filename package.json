{
	"name": "pow-reaction",
	"version": "1.0.17",
	"type": "module",
	"author": "Viktor Shchelochkov <hi@hloth.dev> (https://hloth.dev)",
	"description": "proof-of-work reactions for your blogs",
	"funding": "https://hloth.dev/donate",
	"support": {
		"backing": {
			"npm-funding": true
		}
	},
	"keywords": [
		"svelte",
		"svelte-component",
		"pow",
		"proof-of-work",
		"blog-component"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://git.hloth.dev/hloth/pow-reaction.git"
	},
	"bugs": {
		"url": "https://git.hloth.dev/hloth/pow-reaction/issues"
	},
	"homepage": "https://git.hloth.dev/hloth/pow-reaction#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "rm -rf dist && vite build && bun run prepack",
		"preview": "bun run build && wrangler pages dev",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"deploy": "bun run build && wrangler pages deploy",
		"cf-typegen": "wrangler types src/worker-configuration.d.ts"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.39.8",
		"@sveltejs/kit": "^2.43.7"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.13",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.45.0",
		"vite": "^7.1.9",
		"wrangler": "^4.41.0"
	},
	"dependencies": {
		"@noble/hashes": "^2.0.1",
		"@oslojs/jwt": "^0.3.0",
		"esm-env": "^1.2.2",
		"zod": "^4.1.11"
	}
}